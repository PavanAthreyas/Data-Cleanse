<mvc:View controllerName="cg.pbs.org.DemoRefine.controller.LandingPage" 
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" 
	xmlns="sap.m" 
	xmlns:f="sap.f" 
	xmlns:layout="sap.ui.layout" 
	xmlns:form="sap.ui.layout.form" 
	xmlns:core="sap.ui.core" 
	xmlns:chart="sap.chart" 
	xmlns:data="sap.chart.data">
	<App id="idAppControl">
		<pages>
			<f:FlexibleColumnLayout id="idFlexibleColumnLayout" stateChange="onStateChanged" layout="{/layout}" backgroundDesign="Solid">
				<f:beginColumnPages>
					<mvc:XMLView id="beginView" viewName="cg.pbs.org.DemoRefine.view.UploadPage" />
				</f:beginColumnPages>
			</f:FlexibleColumnLayout>
		</pages>
	</App>
</mvc:View>